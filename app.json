{
  "expo": {
    "name": "gamePlay",
    "slug": "gamePlay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_.png",
    "splash": {
      "image": "./assets/splash_.png",
      "resizeMode": "contain",
      "backgroundColor": "#0E1647"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon_.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon_.png"
    }
  }
}
